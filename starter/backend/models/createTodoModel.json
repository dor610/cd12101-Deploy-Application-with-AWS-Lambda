{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "dueDate": {
        "type": "string",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      }
    },
    "required": ["name","dueDate"],
    "additionalProperties": false
  }