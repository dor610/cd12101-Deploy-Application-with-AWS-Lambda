{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "dueDate": {
        "type": "string",
        "pattern": "^((19|20)\\d{2})-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])$"
      }
    },
    "required": ["name","dueDate"],
    "additionalProperties": false
  }